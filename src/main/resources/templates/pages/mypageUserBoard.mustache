{{>pages/header1}}

<link rel="stylesheet" href="/css/mypageUser.css">
{{>pages/header2}}

<!-- Cotainer는 그렇게 양쪽 여백을 주게 되어있고  fluid 뭐시기는 여백 없음-->
<div class="container">
    <div class="row">

        <section class="mypage_sidebar">
            <nav class="navbar navbar-expand-lg navbar-light bg-light" id="my-page-nav">
                    <button class="navbar-toggler" id="NAVtoggle" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span>목록보기</span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav">
                            {{>pages/mypageUserNAV}}
                        </ul>
                    </div>
            </nav>

            <article class="mypage_article">
                <div class="category_list">
                    <ul class="category_list_wrap">
                        <li><button id="my_board_search" class="active">게시글</button></li>
                        <li><button id="my_commentBoard_search">댓글+게시글</button></li>
                        <li><button id="my_comment_search">댓글</button></li>
                    </ul>
                </div>
                <input type="hidden" id="category_value" value="myboard"/>
                <div class="category">
                    <ul class="category_list_wrap">
                        <li><button id="free">자유게시판</button></li>
                        <li><button id="qna">문의게시판</button></li>
                    </ul>
                </div>
            </article>
        </section>
    </div>
</div>

{{>pages/footer1}}
<!--  이곳에 페이지 별 js 로드시키기-->
