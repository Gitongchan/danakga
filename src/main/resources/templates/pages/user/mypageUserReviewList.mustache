{{>pages/header1}}

<link rel="stylesheet" href="/css/mypageUser.css">
{{>pages/header2}}

<!-- Cotainer는 그렇게 양쪽 여백을 주게 되어있고  fluid 뭐시기는 여백 없음-->
<div class="container">
    <div class="row">

        <section class="mypage_sidebar">
            <nav class="navbar navbar-expand-lg navbar-light bg-light" id="my-page-nav">
                <button class="navbar-toggler" id="NAVtoggle" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span>목록보기</span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        {{>pages/user/mypageUserNAV}}
                    </ul>
                </div>
            </nav>
                <div id="board-table-wrap">
                    <table class="table table-striped table-hover" id="myReviewTable">
                        <colgroup>
                            <col width="10%"/>
                            <col width="50%"/>
                            <col width="10%"/>
                            <col width="20%"/>
                            <col width="10%"/>
                        </colgroup>
                        <thead>
                        <tr>
                            <th>브랜드</th>
                            <th>상품이름</th>
                            <th>작성자</th>
                            <th>시간</th>
                            <th>평점</th>
                        </tr>
                        </thead>
                        <tbody id="myReviewList">
                        </tbody>
                    </table>
                </div>
                <div class="text-center">
                    <ul class="pagination">
                    </ul>
                </div>
        </section>
    </div>
</div>

{{>pages/footer1}}
